  },

  "middleware": {
    "sso": {
      "enabled": true,
      "priority": 108,
      "module": {
        "name": "sso-paypal",
        "arguments": [
          {
            "issuer": "contentmanagernodeweb2023",
            "protocol": "https://",
            "entryPoint": "https://sso.gopaycorp.com.cn:8443/idp/SSO.saml2",
            "exclude": ["/contentmanager/logout"],
            "ssoKeyStore": "node_sso_2023_keystore"
          }
        ]
      }
    },
    "appsec": {
      "enabled": true,
      "priority": 110,
      "module": {
        "name": "lusca",
        "arguments": [
          {
            "csp": {
              "policy": {
                "script-src": "'self' https://*.paypal.com https://*.paypalinc.com https://*.paypalobjects.com https://*.paypal.cn https://*.gopayinc.com.cn https://*.objects.paypal.cn https://*.gopaycorp.com.cn 'unsafe-inline'",
                "img-src": "'self' data: https:",
                "object-src": "'none'",
                "font-src": "'self' data: https://*.paypal.com https://*.paypalinc.com https://*.paypalobjects.com https://*.paypal.cn https://*.gopayinc.com.cn https://*.objects.paypal.cn https://*.gopaycorp.com.cn 'unsafe-inline' 'unsafe-eval'",
                "style-src": "'self' https://*.paypal.com https://*.paypalinc.com https://*.paypalobjects.com https://*.paypal.cn https://*.gopayinc.com.cn https://*.objects.paypal.cn https://*.gopaycorp.com.cn 'unsafe-inline' 'unsafe-eval'",
                "form-action": "'self' https://*.paypal.com https://*.paypalcorp.com https://*.paypalinc.com https://*.paypal.cn https://*.gopayinc.com.cn https://*.paypalcorp.cn",
                "base-uri": "'self' https://*.paypal.com https://*.paypalcorp.com https://*.paypalinc.com https://*.paypal.cn https://*.gopayinc.com.cn https://*.paypalcorp.cn"
              },
              "scriptNonce": true,
              "styleNonce": false
            },
            "csrf": {
              "whitelist": ["/v1/contentmanagerserv"]
            },
            "xframe": "SAMEORIGIN",
            "xssProtection": true
          }
        ]
      }
    }
  }
}
